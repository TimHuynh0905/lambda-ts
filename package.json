{
  "name": "lambda-ts",
  "version": "1.0.0",
  "description": "lambda with typescript template",
  "keywords": [
    "lambda",
    "typescript"
  ],
  "author": "Tim Huynh",
  "license": "ISC",
  "repository": "https://github.com/TimHuynh0905/lambda-ts",
  "scripts": {
    "build": "npm run build:prod && sam validate && sam build",
    "build:prod": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev": "concurrently \"npm run build:dev -- --watch\" \"sam local start-api\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest"
  },
  "dependencies": {
    "aws-sdk": "^2.1238.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/aws-lambda": "^8.10.108",
    "@types/jest": "^29.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.5.0",
    "copy-webpack-plugin": "^11.0.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  }
}
